{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "types": [],
    "lib": ["ES2017", "DOM"],
    "strict": true,
    "verbatimModuleSyntax": true,
    "isolatedModules": true,
    "noUncheckedSideEffectImports": true,
    "moduleDetection": "force",
    "skipLibCheck": true,
  }
}
